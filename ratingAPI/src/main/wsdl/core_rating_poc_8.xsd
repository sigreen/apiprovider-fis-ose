<?xml version="1.0"?>
<!-- edited with XMLSpy v2007 sp2 (http://www.altova.com) by Munjal Kansara (The McGraw-Hill Companies) --><xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://integration.standardandpoors.com/ServiceSchema" xmlns:oa="http://www.openapplications.org/oagis/9" xmlns:sp="http://integration.standardandpoors.com/ServiceSchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:import namespace="http://www.openapplications.org/oagis/9" schemaLocation="core_rating_poc_4.xsd"/>
	<xsd:element name="CreditWatch" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>NEG, POS, DEV, NM</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EffectiveDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>This is the date and time the associated entity is made effective.  eg. if being used in the rating entity, this is the Rating Date in CORE</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ExpirationDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>This is the date and time the associated entity/relationship will be expired.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ReleaseDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>The date when the rating is published.  eg Data Admin Date in CORE</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SignOffDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>The date when the rating is approved by the manager.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CreditOutlook" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>DEVELOPING, NEGATIVE, NM, POSITIVE, STABLE, NR
</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DebtTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>type of debt.  eg. SRSEC, SRUNSEC, SUBOR, PREFDSTK, PRFRCSTK, EQPTRSTCRT</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RatingTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>STDLONG, STDSHORT, FCLONG, FCSHORT, SHADOW</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CurrentRating" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Is this rating current?</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ConfidentialRating" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Is this rating confidential?</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RatingQualifier" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>pi, p, c, f, m</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RatingScore">
		<xsd:annotation>
			<xsd:documentation>AAA, AA, AA-, BBB</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="oa:CodeType">
					<xsd:attribute name="rank" type="xsd:decimal" use="optional"/>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="RatingActionCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>Affirmed, Rating/CW/Outlook changed</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CommitteeTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>the purpose for a rating committee meeting.  eg. New Rating, Asset, Deal, Surveillance</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CommitteeRoleCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>the role an individual may participate in a committee meeting.  Examples include Chair, Voting Member, and Observer.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ClassificationCode" type="oa:IdentifierType">
		<xsd:annotation>
			<xsd:documentation>A Classification code for an entity.  The schemeAgencyId can be used to distinguish the Domain of this Classification code (for example, if there are more than 1 ClassificationCode elements)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ApplicationAttribute" type="oa:IdentifierType">
		<xsd:annotation>
			<xsd:documentation>Application specific attributes associated with an entity.  Use the schemeAgencyId to distinguish Domain of the attribute.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NameAlias">
		<xsd:annotation>
			<xsd:documentation>Other names associated with this entity</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="oa:NameType">
					<xsd:attribute name="type" type="xsd:string" use="optional">
						<xsd:annotation>
							<xsd:documentation>eg. Short Name</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ProgramTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>This code is used to represent what type of program is being used; e.g. CP, CDPRG, etc... if it is not a program then its value will be ISSUE.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IssueTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>BANKNOTE, BONDS, PASSTHRU, NOTES</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CommercialPaperTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>3(A)2, ASIAN, EURO, BRAZIL, TAIWAN, ARGENTINE</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DepartmentCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>COREs rating department code. eg. Structured Finance Ratings, Insurance Ratings, Corporate Ratings, Financial Institutions Ratings</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="GroupCode">
		<xsd:annotation>
			<xsd:documentation>CORE's group code. eg. SERVICE SECTOR, CONSUMER SECTOR, 
CORPORATE FINANCE , HIGH TECHNOLOGY
</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="oa:CodeType">
					<xsd:attribute name="departmentCode" type="xsd:normalizedString" use="optional">
						<xsd:annotation>
							<xsd:documentation>CORE's department code</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Closed" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Is this issue/instrument open or closed?</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PutBond" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Indicates if the issue is a put bond.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Surveillance" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Is this issue undergoes constant surveillance?</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FinalOfferingChecked" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Has the Data Administrator validated the issue using the prospectus?</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="OfficeLocationCode" type="sp:OfficeLocationCodeType">
		<xsd:annotation>
			<xsd:documentation>SandP's office location</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CloseReasonCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>This is a reason for closing the maturity or class</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SubAssetTypeCode">
		<xsd:annotation>
			<xsd:documentation>detailed classification of assets underlying an Issue, e.g., Autos, Manufactured Housing, Student Loans, Recreational Vehicles, and Marine</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="oa:CodeType">
					<xsd:attribute name="assetCategory" type="xsd:normalizedString" use="optional">
						<xsd:annotation>
							<xsd:documentation>asset category code</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="generalAssetType" type="xsd:normalizedString" use="optional">
						<xsd:annotation>
							<xsd:documentation>general asset type code</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="AssetCategoryCode">
		<xsd:annotation>
			<xsd:documentation>intermediate categorization of assets underlying an issue, e.g., "Amortizing Consumer Assets" and "Revolving Consumer Assets"</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="oa:CodeType">
					<xsd:attribute name="generalAssetType" type="xsd:normalizedString" use="optional">
						<xsd:annotation>
							<xsd:documentation>general asset type code</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GeneralAssetTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>high level categorization of a structured finance issue, e.g., ABS, RMBS, CDO</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Name" type="oa:NameType">
		<xsd:annotation>
			<xsd:documentation>Official name or legal name of the entity.  If the entity has other names associated with it, those names should be stored in NameAlias.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Class" type="xsd:normalizedString">
		<xsd:annotation>
			<xsd:documentation>a group of securities with similar features.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MaturityDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>The date on which the principal amount of a note, draft, acceptance bond or other debt instrument becomes due and is repaid to the investor and interest payments stop. It is also the termination or due date on which an installment loan must be paid in full. Not required if evergreen indicator is TRUE</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PlacementTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation> the nature of the market in which an instrument will be issued.  Values include "Public", "144A Registered", and "144A Non-Registered"</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CouponRateTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>Indicates whether an instrument pays a fixed rate of interest or whether the rate floats. Fixed (F), Variable (V) or Zero Coupon (Z)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CouponRate" type="xsd:decimal">
		<xsd:annotation>
			<xsd:documentation>The interest rate stated on a bond when it's issued</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SaleAmount" type="oa:AmountType">
		<xsd:annotation>
			<xsd:documentation>The amount of the instrument at sale time</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SaleDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>The date that the seller of an issue delivers the issue and the buyer pays for it.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CallDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>The date on which a bond can be redeemed before maturity. If the issuer feels there is a benefit to refinancing the issue, the bond may be redeemed on the call date at par or at a small premium to par.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Secured" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Is the instrument secured by collateral to reduce the risk associated with lending?</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SeniorityTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>priority of repayment in case of loss.  Examples include Senior, Subordinated</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SecondaryMarket" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Indicates if a maturity has been issued to the secondary market.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Called" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Indicates if the bond has been called.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Identifier" type="oa:IdentifierType">
		<xsd:annotation>
			<xsd:documentation>Any other known identifiers associated with this entity</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CUSIP" type="oa:IdentifierType">
		<xsd:annotation>
			<xsd:documentation>An identification number assigned to all stocks and registered bonds. The Committee on Uniform Securities Identification Procedures (CUSIP) oversees the entire CUSIP system.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CINS" type="oa:IdentifierType">
		<xsd:annotation>
			<xsd:documentation>An acronym standing for the "CUSIP International Numbering System," which provides identification of international securities.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ISIN" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>A code that uniquely identifies a specific securities issue. The organization that allocates ISINs in any particular country is the National Numbering Agency (NNA).</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TierLevel" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>a code used to group organizations based on certain criteria.  eg. tier 1 (Corporate issuers with &gt; $1 billion of outstanding debt, financial institutions with &gt; $10 billion debt, all Canadian banks, insurance holding companies, all sovereigns)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FiscalYearEnd" type="oa:MonthDateType">
		<xsd:annotation>
			<xsd:documentation>The completion of a one-year, or 12-month, accounting period.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NAIC" type="oa:IdentifierType">
		<xsd:annotation>
			<xsd:documentation>Number issued by National Association of Insurance Commissioner (NAIC) for this Organization - Insurance only</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NextReviewDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>The date of the next comprehensive review of an entity and its rating. </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="LastReviewDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>The date of the last comprehensive review of an entity and its rating. </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="InterimReviewDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>The last interim review date of an entity and its rating. </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EventDrivenReviewDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>The date of event driven review, which can be due to internal event causing review or external party requesting review</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CountryOfRegistration" type="oa:CountryCodeType">
		<xsd:annotation>
			<xsd:documentation>country where the company is registered</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="StateOfRegistration" type="oa:StateCodeType">
		<xsd:annotation>
			<xsd:documentation>State code (NJ, NY), required if country of registration is USA</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RegionCdoe" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>Asia Pacific, Latin America etc</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TickerSymbol" type="oa:IdentifierType">
		<xsd:annotation>
			<xsd:documentation>An arrangement of characters (usually letters) representing a particular security listed on an exchange or otherwise traded publicly. Stock symbols are the most recognized type of ticker symbol.  Other ticker symbols include Bloomberg Ticker, Compustat ticker etc.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IssuerTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>if an organization can act as an Issuer, what type of Issuer it is, e.g., Standard, Conduit, etc. </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FormOfEntityCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>organization's method of legal formation, e.g., Trust, Corporation, LLP, LLC, Partnership, etc.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NACIS" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>North American Industry Classification System (NAICS) code</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SIC" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>U.S. Standard Industrial Classification (SIC) System code</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Active" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>is the entity active within SandP systems?</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="GICSSectorCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>GICS sector code, eg. 10 (Energy)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="GICSIndustryGroupCode">
		<xsd:annotation>
			<xsd:documentation>GICS Industry Group code. eg. 1010 (Energy)</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="oa:CodeType">
					<xsd:attribute name="gicsSectorCode" type="xsd:normalizedString" use="optional">
						<xsd:annotation>
							<xsd:documentation>GICS Sector Code</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GICSIndustryCode">
		<xsd:annotation>
			<xsd:documentation>GICS Industry code. eg. 101010 (Energy Equipment and Services)</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="oa:CodeType">
					<xsd:attribute name="gicsIndustryGroupCode" type="xsd:normalizedString" use="optional">
						<xsd:annotation>
							<xsd:documentation>GICS Industry Group Code</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="gicsSectorCode" type="xsd:normalizedString" use="optional">
						<xsd:annotation>
							<xsd:documentation>GICS Sector Code</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GICSSubIndustryCode">
		<xsd:annotation>
			<xsd:documentation>GICS Sub-Industry code.  eg. 10101010 (Oil and Gas Drilling) 10101020 (Oil and Gas Equipment and Services)</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="oa:CodeType">
					<xsd:attribute name="gicsIndustryCode" type="xsd:normalizedString" use="optional">
						<xsd:annotation>
							<xsd:documentation>GICS Industry Code</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="gicsIndustryGroupCode" type="xsd:normalizedString" use="optional">
						<xsd:annotation>
							<xsd:documentation>GICS Industry Group Code</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="gicsSectorCode" type="xsd:normalizedString" use="optional">
						<xsd:annotation>
							<xsd:documentation>GICS Sector Code</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ParticipationRoleCode">
		<xsd:annotation>
			<xsd:documentation>organizations, instruments or internal support may participate with Issues, Instruments, or Collateral Groups.  Organization roles include Issuer, Guarantor, Bond Insurer, Bond Counsel, and LOC Provider etc.  Instrument roles include Subordinated Class and Guarantee Bond etc. Internal Support roles include overcollateralization, excess spread, and reserve fund.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="oa:CodeType">
					<xsd:attribute name="participationTypeCode" type="xsd:normalizedString" use="optional">
						<xsd:annotation>
							<xsd:documentation>type of participation, organization, instrument or internal support</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Confidential" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>is this confidential?</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="LiabilityTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>The type of liability entered into as per the following definitions:

(1) Joint and Several: Where two or more parties enter into an obligation such as a guarantee together, joint and several liability means that the lender or creditor can recover the whole indebtedness from any one of them. They are then left to sort out their respective contributions between themselves. 

(2) Several: When members of a group agree to certain obligations individually, but do not share responsibility for the obligations of other members of the group.

(3) Joint: Pertaining to multiple parties on the same side of an agreement</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EnhancesLiabilities" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Indication if support enhances Liabilities (instruments)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EnhancesAssets" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Indication if support enhances Assets (collateral groups)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ForCredit" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Indication if support is for enhancing credit</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ForLiquidity" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Indication if support is for enhancing liquidity</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SupportDenominatorCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>the type of value to be used as a denominator in an equation that calculates a type percentage covered.  Known examples are "Invested Amount", "Outstanding Collateral Amount"</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SwapTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>Traditionally, the exchange of one security for another to change the maturity (bonds), quality of issues (stocks or bonds), or because investment objectives have changed. Recently, swaps have grown to include currency swaps and interest rate swaps.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Comment" type="oa:NoteType"/>
	<xsd:complexType name="OfficeLocationCodeType">
		<xsd:simpleContent>
			<xsd:extension base="oa:CodeType">
				<xsd:attribute name="countryCode" type="xsd:normalizedString" use="optional">
					<xsd:annotation>
						<xsd:documentation>Country Code</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="stateCode" type="xsd:normalizedString" use="optional">
					<xsd:annotation>
						<xsd:documentation>State Code</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:element name="OrganizationTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>CORE organization type, eg. HOMEFURN  (Home Furnishings), HOMEBUILD (Homebuilders), HOTELGAMCO (Gaming Cos)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SectorCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>RatingsDirect Sector code. eg GLOBISS (Global Issuer), STRUC (Structured Finance), PUBFIN (US Public Finance)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SubSectorCode">
		<xsd:annotation>
			<xsd:documentation>RatingsDirect sub-sector code.  CORP (Corporations), FI (Financial Institutions), INS (Insurance), ABS (Asset Backed Securities)</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="oa:CodeType">
					<xsd:attribute name="sectorCode" type="xsd:normalizedString" use="optional">
						<xsd:annotation>
							<xsd:documentation>RD sector code</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="IndustryCode">
		<xsd:annotation>
			<xsd:documentation>RatingsDirect Industry code. 1 (Aerospace and Defense), 2 (Automotive), FINANCEGCO (Financial Guaranty), ASSTMNGR (Asset Manager), BANK (Bank) etc.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="oa:CodeType">
					<xsd:attribute name="subSectorCode" type="xsd:normalizedString" use="optional">
						<xsd:annotation>
							<xsd:documentation>RD sub-sector code</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="sectorCode" type="xsd:normalizedString" use="optional">
						<xsd:annotation>
							<xsd:documentation>RD sector code</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="RatingRequestDate" type="xsd:date">
		<xsd:annotation>
			<xsd:documentation>The date upon which the lead analyst begins work on rating the entity.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="InstrumentTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>the type of financial instruments that are issued as a result of a deal to create an Issue. eg. Term, Medium Term Note (MTN), and Commercial Paper (CP).</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CertificateGroupName" type="oa:NameType">
		<xsd:annotation>
			<xsd:documentation>Code/Word or Codes/words describing and distinguishing a certificate group from others.  For structured residential mortgage backed securities, and possibly other types, each instrument of an issue may be assigned to a certificate group.  There may be more than one certificate group per issue.  Certificate groups will generally initially have an autonomous subordination structure.  However embedded deal triggers may cause a crossover of subordination from one certificate group to another when the most junior tranches of one certificate group have been depleted.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RatingGroupName" type="oa:NameType">
		<xsd:annotation>
			<xsd:documentation>Code/Word or Codes/words describing and distinguishing a rating group from others.  A rating group is assigned to each instrument in an issue, and subordination may then be calculated as per the rating groups and if applicable, the Certificate Groups.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CashFlowTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>type of cash flow.  Known values are "Principle and Interest", "Principle", and "Interest"</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NotionalAmount" type="oa:AmountType">
		<xsd:annotation>
			<xsd:documentation>in an interest rate swap, the predetermined dollar amount on which the exchanged interest payments are based.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RemitStartDate" type="xsd:date">
		<xsd:annotation>
			<xsd:documentation>The first date on which a payment (P, I, or P and I, depending on the instrument) is due.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ExpectedMaturityDate" type="xsd:date">
		<xsd:annotation>
			<xsd:documentation>The expected maturity is the date as of which the securities are “expected” to be repaid in full based on a specified assumption regarding the rate at which the underlying assets will be prepaid, but there is usually no guarantee that repayment will not occur either before or after that date.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FrequencyTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>frequency for a payment or some other regular event.  Examples include daily, weekly, bi-monthly, monthly, quarterly, semi-annually, annually.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AverageBondLife" type="xsd:decimal">
		<xsd:annotation>
			<xsd:documentation>A measure of the duration of an investment, based on the average length of time required to get back the principal amount invested; calculated by multiplying each repayment of principal by the time elapsed between making the investment and receiving the principal repayment, summing the results, and dividing by the total amount invested</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PaidOffDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>This is the date the instrument's outstanding principal balance goes to zero.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CollateralGroupNumber" type="xsd:integer">
		<xsd:annotation>
			<xsd:documentation>A user-defined number for a collateral group.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CollateralGroupName" type="oa:NameType">
		<xsd:annotation>
			<xsd:documentation>A user defined word or words used to designate a collateral group.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="StateCode">
		<xsd:annotation>
			<xsd:documentation>States or Provinces</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="oa:StateCodeType">
					<xsd:attribute name="countryCode" type="xsd:normalizedString">
						<xsd:annotation>
							<xsd:documentation>country where this state/province located</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="CollateralGroupRelationshipCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>Code used to uniquely identify a collateral group relationship type. eg. Shares collateral with</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IndexTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>For a variable rate instrument, the specific index type upon which the coupon is calculated.  Examples include "1 Year LIBOR", "Prime", "11th District COFI", and '1 Year CMT"</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MarginRate" type="xsd:decimal">
		<xsd:annotation>
			<xsd:documentation>A rate, when added to the current rate of the index indicated by the index_type_code, gives the total rate of interest of the instrument.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="LifetimeCapitalizationRate" type="xsd:decimal">
		<xsd:annotation>
			<xsd:documentation>The highest value the interest rate of this instrument may be for the life of the instrument.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="LifetimeFloorRate" type="xsd:decimal">
		<xsd:annotation>
			<xsd:documentation>The lowest value the interest rate of this instrument may be for the life of the instrument.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CommercialPaperEvergreen" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Does this CP have an expiration date (N- default) or is evergreen(Y) ? Extensions are handled by updating the maturity date</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PolicyNumber" type="oa:IdentifierType">
		<xsd:annotation>
			<xsd:documentation>This is the policy number associated with commercial paper.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AuthorizedAmount" type="oa:AmountType">
		<xsd:annotation>
			<xsd:documentation>This is the amount authorized for this Commercial Paper.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Taxable" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>is the return on this security subject to taxes at the local, state or federal level, or some combination thereof.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DatedDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>The date at which interest begins to accrue on a fixed-income security.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Refunded" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>has the associated entity refunded?</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RefundDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>this is the date the associated entity has been refunded.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RatingAgencyCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>Rating agency who determined the ratings. eg. SandP, Moody's and Fitch.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RatingDomainCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>Indicates the domain to which a rating applies.  eg.  Local Currency, Foreign Currency.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ReasonTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>Reason for a rating.  Examples are new, review, affirmation, called, matured.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NationalScaleCode">
		<xsd:annotation>
			<xsd:documentation>Assessment scale relative within a specific sovereign rather than absolute globally.  Examples are Brazilian Rating Scale, Canadian National Scale, and Canadian CP Rating Scale.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="oa:CodeType">
					<xsd:attribute name="prefix" type="xsd:normalizedString" use="optional">
						<xsd:annotation>
							<xsd:documentation>Two positions of text that will be displayed with any National Scale Rating.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="countryCode">
						<xsd:annotation>
							<xsd:documentation>country code to which this national scale applies</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="RatingStatusCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>state of a released rating. eg. Preliminary and Final.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ShadowRating" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>indicating whether this rating applies to the enhanced debt (N) or to the underlying debt as if it were not enhanced (Y), in the case where the rated entity is enhanced externally</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PublishingAgencyCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>This is the Rating agency who published the rating, but not necessarily the one who determined the rating.  As an example, Bloomberg sometimes publishes SandP's Public Finance Ratings BEFORE SandP does, tagging them as "Expected".  In this case, the publishing agency code will be Bloomberg the source rating agency will be Standard and Poor's and the expected flag will be set to "TRUE".</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AnalystRoleCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>analyst role, eg. primary analyst, backup analyst etc.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EmailAddress">
		<xsd:annotation>
			<xsd:documentation>email address associated with the entity. eg. business email, personal email etc.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="xsd:normalizedString">
					<xsd:attribute name="type" type="xsd:normalizedString">
						<xsd:annotation>
							<xsd:documentation>email address types. eg. business, personal etc.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Pending" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>is the associated entity in pending state?</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AssessmentTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>code used to indicate type of rating assessment, such as SHADOW, SPUR etc</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DealNumber" type="xsd:normalizedString">
		<xsd:annotation>
			<xsd:documentation>this is the number related to a deal or issue</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CoverageTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>code used to denote a type of coverage</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BalanceAmount" type="oa:AmountType">
		<xsd:annotation>
			<xsd:documentation>the balance still owed by the associated entity</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BalanceDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>the date on which the balance amount is updated with the associated entity</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FinancialsDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>the date financial performace paper will be published.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AnticipatedClosingDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>collateral manager estimated date for the deal closing.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AnalysisTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>Code used to uniquely identify the type of analysis performed.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="WorkRoleCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>Code used to uniquely identify a Work Role. </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EvaluationRequestDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>The date upon which the client requested this evaluation.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RecoveryRate" type="xsd:decimal">
		<xsd:annotation>
			<xsd:documentation>The rate of recovery rate that will be applied on the asset in case of default.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="HyperTranching" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Indicates whether the CMBS structured security is hyper tranched or not.  A 'Y' inidicates that this particular tranche is involved in other deals.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="OriginalTermOfPayments" type="xsd:decimal">
		<xsd:annotation>
			<xsd:documentation>The number of months expected to pay off principle and interest of the instrument.  Can be calculated from the (expected maturity date minus remit start date) divided by pay frequency.  For CE instruments only the number of payments is captured.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AmortizingTermOfPayments" type="xsd:decimal">
		<xsd:annotation>
			<xsd:documentation>The number of months of the orginal_term_of_paymens that will be paying off the principle and not just interest.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="InitialRatingSoldAmount" type="oa:AmountType">
		<xsd:annotation>
			<xsd:documentation>The amount of the Instrument's initial principal balance amount that is sold as of the initial rating.  The par amount of the instrument and is normally the same as the initial principal balance.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SurveillanceRequired" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>true indicates that this requires a followup serveillance evaluation.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FeeAmount" type="oa:AmountType">
		<xsd:annotation>
			<xsd:documentation>true indicates that this requires a followup serveillance evaluation.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BillingDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>The date that the bill for the fee is to be sent.  Usually the first of the month.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CompleteReviewDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>This is the last full evaluation date.  An evaluation may be reused, if the timespan from the last full evaluation date is within an acceptable range and no major changes have occurred.  If an old evaluation is not reused, then this will match the Rating.EffectiveDate</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="LoanPieceCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>Code used to uniquely identify a Loan Piece.  Please see Loan Piece entity definition for further details and examples.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PropertyTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>Code used to uniquely identify a Property Type.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DebtServiceCoverageRatio" type="xsd:decimal">
		<xsd:annotation>
			<xsd:documentation>The DSC is the ratio of cash flow available from the property relative to the annual interest and principal payments of the loan.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NetCashFlowAmount" type="oa:AmountType">
		<xsd:annotation>
			<xsd:documentation>The NCF is the net cash flow per year from the asset</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="LoanToValueRatio" type="xsd:decimal">
		<xsd:annotation>
			<xsd:documentation>The loan amount divided by the stabalized amount (stressed value).</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CapitalizationPercentage" type="xsd:decimal">
		<xsd:annotation>
			<xsd:documentation>The face amount purchased divided by the stabalized value (stressed sale)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="StabalizedValueAmount" type="oa:AmountType">
		<xsd:annotation>
			<xsd:documentation>The valuation assigned to real estate collateral estimating the value at a 'stressed' sale.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="InitialPrincipleBalanceAmount" type="oa:AmountType">
		<xsd:annotation>
			<xsd:documentation>The initial amount borrowed by the issuer of debt from the instrument-holder.  Also known as initial face value.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NextSurveillanceFeeAmount" type="oa:AmountType">
		<xsd:annotation>
			<xsd:documentation>The fee charged for in advance for the next surveillance.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="GeneralIssueTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>High level categorization of issue.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="GVKey" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>Compustat has defined a proprietary identifier, the GVKEY, for each company in the database. The GVKEY can be used to track a company over time, while the company name, CUSIP, or ticker may change over time.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- Newly Added Elements for Program Schema for Volume Project 10-Feb-2007 -->
	<xsd:element name="ProgramRelationID" type="oa:IdentifierType">
		<xsd:annotation>
			<xsd:documentation>Program RelationShip ID for Issue fto Program Amounts</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Amount" type="oa:AmountType">
		<xsd:annotation>
			<xsd:documentation>The dollar amount for the country. Program Amount for the Issue Level Reference</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AmountText" type="xsd:normalizedString">
		<xsd:annotation>
			<xsd:documentation>Text Amount for Program Level Reference</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IssueID" type="oa:IdentifierType">
		<xsd:annotation>
			<xsd:documentation>Issue which the Program references for Relationship</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RegistrationDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>Date of the Registration of the Program for their Organization</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ClosedDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>Closed Date of the Program for the Organization Rating</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Completed" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>This indicates whether the data manager has finished entering the data and would like DAD to sign off.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IsMultiDepts" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Used for determining if instruments (programs, instruments) present are from multiple departments or one department 
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="UnlimitedAmount" type="xsd:normalizedString">
		<xsd:annotation>
			<xsd:documentation>Unlimited Amount of the Program for the Issuing Organization</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Evergreen" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation> Does this have an expiration date (N- default) or is evergreen(Y) ? Extensions are handled by updating the maturity date</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Suspect" type="xsd:normalizedString">
		<xsd:annotation>
			<xsd:documentation>This indicates if there is suspect data associated with the row. </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ProgramID" type="oa:IdentifierType">
		<xsd:annotation>
			<xsd:documentation>SHELF_ID/ProgramID This is a unique identifier for the shelf which approved this issuing</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ProgramRelationshipTypeCode" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>Program Relation Type code with the issue.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Issue_Market" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>MARKET_TRADE_CODE or the Foreign Market Code of the Issue Occurence
			This is the place of exchange</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Redemption" type="xsd:normalizedString">
		<xsd:annotation>
			<xsd:documentation>Redemption Value of the Issue Occurence</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="UnsolicitedRating" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Un Solicited Rating Value for the Program Related Issues</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Replenishment" type="xsd:normalizedString">
		<xsd:annotation>
			<xsd:documentation>Replenishment for the program occurence within Organization</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Series" type="xsd:normalizedString">
		<xsd:annotation>
			<xsd:documentation>Series Deinifiton ACH,PRG
			Identifier of series</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AnalystDate" type="oa:DateTimeType">
		<xsd:annotation>
			<xsd:documentation>Date of analyst's action</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PendingStatusInd" type="oa:CodeType">
		<xsd:annotation>
			<xsd:documentation>If Data Administration has not yet performed a quality check, the value will be P = Pending; if this has occurred successfully, Data Administration will change the status to R for Released. A delayed release will be H for Held. Default is Pending(P).</xsd:documentation>
		</xsd:annotation>
